CREATE TABLE opioid__search_rules_descriptions AS 
    SELECT 
        e.tty1,
        e.rela,
        e.tty2,
        e.rule,
        e.include,
        u1.tty_str AS tty1_str,
        u2.tty_str AS tty2_str
    FROM
        opioid__umls_tty as u1,
        opioid__umls_tty as u2,
        opioid__search_rules as e
    WHERE
        u1.tty = e.tty1
        AND u2.tty = e.tty2
